    "homepage": "https://github.com/davidtsadler/ebay-sdk-trading",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "David T. Sadler",
            "email": "davidtsadler@googlemail.com",
            "homepage": "http://davidtsadler.com"
